{
  "name": "kamukunji-business-arcade",
  "version": "1.0.0",
  "description": "Digital marketplace for Kamukunji Business Arcade",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "echo 'Build handled by Dockerfile'",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "start:prod": "NODE_ENV=production node server/index.js",
    "build:prod": "npm run build && npm run start:prod",
    "heroku-postbuild": "echo 'Build handled by Dockerfile'"
  },
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.22.0",
    "@supabase/supabase-js": "^2.76.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "business",
    "marketplace",
    "shoes",
    "arcade"
  ],
  "author": "Kamukunji Business Arcade",
  "license": "MIT"
}
